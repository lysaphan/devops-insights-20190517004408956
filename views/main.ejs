    <script>
      var map;
      function initMap() {
      	
      	var Hamilton = {lat: -37.78333, lng: 175.28333};
      	
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: -41.811244, lng: 173.687056},
          zoom: 5
      
        });
        
        var marker = new google.maps.Marker({position: Hamilton, map: map});
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC22hm5Rl_JgliDoxIcY6t8AmjlrrS33cw&callback=initMap"
    async defer></script>
        
